<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/personalinfo"
        app:navigationIcon="@drawable/goback_black"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/main_gray_line"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/state"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingming"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/idcard"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>

            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/phonenum"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shenfenzhengzhengmian"/>

            </LinearLayout>
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginHorizontal="20dp"
                    android:scaleType="fitCenter"
                    android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shenfenzhengfenmian"/>

            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shouchishenfenzheng"/>

            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:background="@color/main_gray_line"/>
            <View
                android:layout_width="match_parent"
                android:layout_marginTop="15dp"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/chepaihao"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/chexing"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingshizheng"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/jiashizheng"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/suoyouren"/>
                <TextView
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingshizhengzhengmianzhaopian"/>

            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/jiashizhengzhengmianzhaopian"/>

            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"/>

            <TextView
                android:id="@+id/tv_commit"
                style="@style/ButtonStyle"
                android:layout_marginHorizontal="20dp"
                android:layout_marginVertical="40dp"
                android:text="确定"
                android:textSize="16sp"/>


        </LinearLayout>
    </ScrollView>
</LinearLayout>