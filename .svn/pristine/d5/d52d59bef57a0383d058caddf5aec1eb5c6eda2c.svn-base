<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/personalinfo"
        app:navigationIcon="@drawable/goback_black"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/main_gray_line"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/state"/>
                <TextView
                    android:id="@+id/tv_state"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingming"/>
                <TextView
                    android:id="@+id/tv_name"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/idcard"/>
                <TextView
                    android:id="@+id/tv_idcard"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>

            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/phonenum"/>
                <TextView
                    android:id="@+id/tv_phonenum"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shenfenzhengzhengmian"/>

            </LinearLayout>
                <ImageView
                    android:id="@+id/img_idcardimg1"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginHorizontal="20dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_normal_pic_default"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shenfenzhengfenmian"/>

            </LinearLayout>
            <ImageView
                android:id="@+id/img_idcardimg2"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/icon_normal_pic_default"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/shouchishenfenzheng"/>

            </LinearLayout>
            <ImageView
                android:id="@+id/img_idcardimg3"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/icon_normal_pic_default"/>
            <View
                android:layout_width="match_parent"
                android:layout_marginTop="15dp"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/chepaihao"/>
                <TextView
                    android:id="@+id/tv_carnum"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/chexing"/>
                <TextView
                    android:id="@+id/cartype"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingshizheng"/>
                <TextView
                    android:id="@+id/tv_xingshizheng"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/jiashizheng"/>
                <TextView
                    android:id="@+id/tv_jiashizheng"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/suoyouren"/>
                <TextView
                    android:id="@+id/tv_carsuoyouren"
                    style="@style/TextViewStyle"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:textColor="@color/black_666"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/main_gray_line"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/xingshizhengzhengmianzhaopian"/>

            </LinearLayout>
            <ImageView
                android:id="@+id/img_xingshizhengimg1"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/icon_normal_pic_default"/>
            <LinearLayout
                style="@style/LinelayoutStyle"
                android:paddingHorizontal="20dp">
                <TextView
                    style="@style/TextViewStyle"
                    android:text="@string/jiashizhengzhengmianzhaopian"/>

            </LinearLayout>
            <ImageView
                android:id="@+id/img_jiashiimg1"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginHorizontal="20dp"
                android:src="@drawable/icon_normal_pic_default"/>
            <Space
                android:layout_width="match_parent"
                android:layout_height="20dp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>